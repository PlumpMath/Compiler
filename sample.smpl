function ain() :bool 
{
    return 1>2;
}


function fact(n: int) :int 
{
    var x : int;
    if((n==0) || (n==1)){
        x = 1;
    }
    else{
        x= fact(n-1);
        x = n*x;
    }

    return x;
}

function Main() :int 
{
    var x:int;
    x = fact(4);
    if(true){
        x=2;
    }
    else{
        x=|-10|;
    }
    return  x;
}



